<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    <record id="ir_cron_car_expiry_reminders" model="ir.cron">
        <field name="name">CarReminders</field>
        <field name="model_id" ref="odoo_asset_manager.model_assets_car"/>
        <field name="state">code</field>
        <field name="code">model._cron_send_expiry_reminders()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="active" eval="True"/>
        <field name="nextcall"
                  eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 12:00:00')"/>
        <field name="priority">1</field>
    </record>
    </data>
</odoo>